{
  "name": "e-commerce",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "build:affected": "npx env-cmd -f .env.prod nx affected --target=build",
    "build-all": "npx env-cmd -f .env.prod nx run-many --target=build --projects=auth,client --parallel",
    "deploy:digital": "npx env-cmd -f .env.prod nx run-many --target=build-docker --projects=auth,client --parallel",
    "test": "nx affected --target=test",
    "test:watch": "npx env-cmd -f .env.dev nx affected --target=test --watch",
    "start-infra:dev": "docker-compose -f infra/docker/docker-compose.dev.yml up && yarn run stop-deploy:minikube",
    "start-apps:dev": "npx env-cmd -f .env.dev nx run-many --target=serve --projects=auth,client --parallel",
    "deploy:minikube": "sh infra/k8s-dev/deploy-minikube.sh",
    "stop-deploy:minikube": "minikube stop",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:stop": "docker-compose stop"
  },
  "private": true,
